#include <stdio.h>

int main()                                          
{
    int a[9][9];
	int x, y;
	for (int i = 0; i < 9; i++)
	{
		for (int j = 0; j < 9; j++)
		{
			a[i][j] = (i + 1) * (j + 1);
		}

	}
	printf("알고싶은 구구단을 입력하시오 : ");
	scanf_s(" %d %d", &x, &y);
	printf("%d * %d = %d", x, y, a[x-1][y-1]);
}
