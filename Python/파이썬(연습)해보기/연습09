x = ["R", "A", "L"]   #리스트
print(x)


x.append("C")  #맨뒤에 리스트에 추가
print(x)



x.insert(2, "M")  #리스트 사이에 L자리에 나오고 그 뒤는 뒤로 밀림
print(x)


x.pop()  #맨뒤를 빼고 리스트 출력 -> c사라짐
print(x)




s = ["이", "최", "김", "이"]
print(s)
print(s.count("이"))  #몇번 등장




N = [5,2,3,1,4,6]
N.sort()  #숫자 정렬
print(N)


N.reverse()   #반대로
print(N)




N.clear()   #모든 리스트 삭제
print(N)



N.extend(x)  #리스트 합치기
print(N)


N.extend(s)  #리스트 합치기
print(N)
