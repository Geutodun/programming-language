for W in [1,2,3,4,5]:         #반복문
    print("대기번호 : {0}".format(W))



for R in range(6):  #0 1 2 3 4 5
    print("가격 : {0}만원".format(R))


for R in range(1, 7): #1 2 3 4 5 6
    print("가격 : {0}십만원".format(R))



R = ["키보드", "마우스", "헤드셋", "모니터", "케이스"]  #리스트 반복
for product in R:
    print("{0}, 제품이 준비 되었습니다.".format(product))
